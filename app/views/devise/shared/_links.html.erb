<%- if controller_name != 'sessions' %>
  <%= link_to I18n.t("views.login.log_in"), new_session_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to I18n.t("views.shared.registrations"), new_registration_path(resource_name) %><br />
<% end %>